      <div id="LD">
      <h3>LD [POST]</h3>
      </div>
      <p>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Order</th>
              <th scope="col">Write</th>
              <th scope="col">Detail</th>
              <th scope="col">OP</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th rowspan="2">Load</th>
              <td>LD r1, r2</td>
              <td>r1 ← (r2)</td>
              <td>0x14</td>
            </tr>
            <tr>
              <td>LD r1, adr [,x]</td>
              <td>r1 ← (実行アドレス)</td>
              <td>0x10</td>
            </tr>
          </tbody>
        </table>
      </p>
      <h3>Attributes</h3>
      <p>code: (string,optional) - CASL2 Source Code</p>
      <h3>Exsample 1</h3>
      <div class="card">
        <div class="card-body">
        <code>
          {
            "code": "LD GR1,GR2"
          }
        </code>
        </div>
      </div>
      <h3>Response 200 (application/json)</h3>
      <div class="card">
        <div class="card-body">
        <code>
        {
          "result":"OK",
          "code" :[
            {
              "Code":5138,
              "Addr":0,
              "AddrLabel":"",
              "Op":20,
              "Length":1,
              "Token":{
                  "Literal":"LD"
              }
            }
          ]   
          }
        </code>
        </div>
        <h3>Exsample Request2</h3>
        <div class="card">
        <div class="card-body">
        <code>
          {
            "code": "LD GR1,0100"
          }
        </code>
        </div>
      </div>
      <h3>Response 200 (application/json)</h3>
      <div class="card">
        <div class="card-body">
        <code>
        {"result":"OK","code" :[{"Code":4112,"Addr":64,"AddrLabel":"","Op":16,"Length":2,"Token":{"Literal":"LD"}}]}
        </code>
        </div>

      </div>

